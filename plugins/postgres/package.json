{
  "name": "@tago-io/tcore-plugin-postgresql",
  "version": "0.7.0",
  "private": false,
  "main": "./build/index.js",
  "files": [
    "build/**/*",
    "assets/**/*"
  ],
  "engines": {
    "tcore": ">= 0.7.0 < 0.8.0",
    "node": ">=14.17.0"
  },
  "tcore": {
    "name": "PostgreSQL",
    "short_description": "Integrates the PostgreSQL Database with TagoCore",
    "full_description": "./README.md",
    "icon": "./assets/icon.png",
    "cluster": true,
    "types": [
      "database"
    ],
    "permissions": [
      "action",
      "analysis",
      "device-data",
      "device"
    ]
  },
  "scripts": {
    "pack": "tcore-plugin pack",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "node esbuild/build.js --watch",
    "build": "rm -rf ./build; tsc; node esbuild/build.js",
    "linter": "eslint src",
    "migrate": "knex migrate:make --client=pg --migrations-directory=src/Migrations"
  },
  "dependencies": {
    "@tago-io/tcore-sdk": "0.6.1-alpha.0",
    "esbuild": "0.14.29",
    "knex": "^0.95.10",
    "pg": "^8.7.3",
    "pg-native": "^3.0.0",
    "pg-query-stream": "^4.2.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.4.1",
    "@types/node": "16.7.1",
    "@types/pg": "^8.6.5",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "axios": "^0.27.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "luxon": "^2.3.2",
    "prettier": "^2.5.1",
    "qs": "^6.10.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  }
}
