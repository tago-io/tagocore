{
  "name": "@tago-io/tcore-plugin-filesystem-s3",
  "version": "0.7.0",
  "private": true,
  "main": "./build/index.js",
  "tcore": {
    "name": "AWS S3 Filesystem",
    "icon": "./assets/icon.png",
    "short_description": "Use an AWS S3 Bucket as a filesystem",
    "full_description": "./README.md",
    "cluster": true,
    "types": ["filesystem"],
    "permissions": []
  },
  "engines": {
    "tcore": ">= 0.7.0 < 1.0.0"
  },
  "files": [
    "assets/**",
    "build/**"
  ],
  "scripts": {
    "pack": "tcore-plugin pack",
    "watch": "node esbuild/build.js --watch",
    "linter": "eslint .",
    "build": "npm run linter; rm -rf ./build; node esbuild/build.js"
  },
  "dependencies": {
    "aws-sdk": "2.1105.0",
    "@tago-io/tcore-sdk": "0.5.0"
  },
  "devDependencies": {
    "typescript": "4.6.4",
    "@types/node": "^16.7.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "esbuild": "^0.14.38",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2"
  }
}
