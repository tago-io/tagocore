{
  "name": "@tago-io/tcore-plugin-rabbitmq",
  "version": "0.7.0",
  "private": false,
  "main": "./build/index.js",
  "engines": {
    "tcore": ">= 0.7.0 < 0.8.0"
  },
  "files": [
    "build/**/*",
    "assets/**/*"
  ],
  "tcore": {
    "name": "RabbitMQ",
    "short_description": "Integrates the RabbitMQ queue with TagoCore",
    "full_description": "./README.md",
    "icon": "./assets/icon.png",
    "types": [
      "queue"
    ],
    "permissions": [
      "device-data"
    ],
    "publisher": {
      "name": "TagoIO",
      "domain": "tago.io"
    }
  },
  "scripts": {
    "pack": "tcore-plugin pack -f -o tcore-plugin-rabbitmq.tcore",
    "watch": "tsc --watch",
    "build": "rm -rf ./build; node esbuild/build.js",
    "linter": "eslint src"
  },
  "dependencies": {
    "@tago-io/tcore-sdk": "0.6.1-alpha.0",
    "amqplib": "^0.10.2",
    "knex": "0.95.15",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/node": "16.7.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "esbuild": "^0.14.49",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1"
  }
}
