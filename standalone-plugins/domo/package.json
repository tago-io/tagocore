{
  "name": "@tago-io/tcore-plugin-domo",
  "version": "0.6.0",
  "private": false,
  "main": "./build/index.js",
  "engines": {
    "tcore": ">= 0.4.0 <= 1.0.0"
  },
  "files": [
    "assets/**",
    "build/**"
  ],
  "tcore": {
    "name": "Domo",
    "short_description": "Enables integration between Domo and TagoCore",
    "full_description": "./README.md",
    "icon": "./assets/icon.png",
    "types": [
      "action-type",
      "action-trigger",
      "service"
    ],
    "permissions": [
      "device",
      "device-data"
    ]
  },
  "scripts": {
    "pack": "tcore-plugin pack",
    "test": "echo No Test",
    "watch": "node esbuild/build.js --watch",
    "build": "rm -rf ./build; node esbuild/build.js"
  },
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "@tago-io/tcore-sdk": "0.7.2",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/node": "16.7.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "esbuild": "^0.14.26",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1"
  }
}
