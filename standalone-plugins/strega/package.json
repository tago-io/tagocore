{
  "name": "tcore-plugin-strega",
  "version": "0.7.0",
  "description": "",
  "main": "./build/index.js",
  "engines": {
    "tcore": ">= 0.7.0 < 0.8.0"
  },
  "files": [
    "build/**/*",
    "assets/**/*"
  ],
  "scripts": {
    "pack": "tcore-plugin pack",
    "watch": "node esbuild/build.js --watch",
    "linter": "eslint .",
    "test": "jest",
    "build": "npm run linter; rm -rf ./build; node esbuild/build.js"
  },
  "tcore": {
    "name": "Strega",
    "short_description": "Adds support for LoRaWAN Strega sensor encoders",
    "full_description": "./README.md",
    "icon": "./assets/icon.png",
    "cluster": true,
    "types": [
      "encoder"
    ],
    "permissions": [
      "device-data"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.21",
    "esbuild": "^0.14.23",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "@tago-io/tcore-sdk": "0.7.2",
    "@types/jest": "^27.5.1",
    "jest": "^28.1.0"
  }
}
