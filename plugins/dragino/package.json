{
  "name": "tcore-plugin-dragino",
  "version": "0.0.1",
  "description": "",
  "main": "./build/index.js",
  "engines": {
    "tcore": ">= 0.4.0 < 0.5.0"
  },
  "files": [
    "build/**/*",
    "assets/**/*"
  ],
  "scripts": {
    "pack": "tcore-plugin pack",
    "watch": "node esbuild/build.js --watch",
