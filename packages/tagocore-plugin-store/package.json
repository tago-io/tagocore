{
  "name": "@tago-io/tagocore-plugin-store",
  "version": "0.7.0",
  "private": true,
  "main": "./build/back/index.js",
  "engines": {
    "tcore": ">= 0.7.0 < 0.8.0"
  },
  "files": [
    "assets/**/*",
    "build/**/*"
  ],
  "scripts": {
    "build": "node esbuild/build.js",
    "watch": "node esbuild/build.js --watch",
    "pack": "../../node_modules/.bin/tcore-plugin pack --filename 2e8e52b993bcfbb495f0e4b51f687d89.tcore"
  },
  "tcore": {
    "name": "Plugin List",
    "short_description": "Plugin list plugin",
    "full_description": "./README.md",
    "permissions": [
      "cli"
    ],
    "icon": "./assets/icon.png",
    "cluster": true,
    "hidden": true,
    "types": [
      "sidebar-button",
      "page",
      "service"
    ],
    "cli": {
      "commands": [
        {
          "name": "plugin-install",
          "description": "Installs one or more Plugins",
          "arguments": [
            {
              "flags": "<id...>",
              "description": "Plugin Slugs, IDs, or File path"
            }
          ],
          "file": "./build/cli/index",
          "handler": "installPlugins"
        },
        {
          "name": "plugin-search",
          "description": "Searches for a Plugin",
          "arguments": [
            {
              "flags": "<filter>",
              "description": "Plugin Slugs, IDs or Description"
            }
          ],
          "file": "./build/cli/index",
          "handler": "searchPlugins"
        },
        {
          "name": "plugin-versions",
          "description": "Lists all versions of a Plugin",
          "arguments": [
            {
              "flags": "<id...>",
              "description": "Plugin Slugs, IDs, or File path"
            }
          ],
          "file": "./build/cli/index",
          "handler": "listPluginsVersions"
        },
        {
          "name": "plugin-list",
          "description": "Lists all installed plugins",
          "file": "./build/cli/index",
          "handler": "listPlugins"
        },
        {
          "name": "plugin-uninstall",
          "description": "Uninstalls one or more Plugins",
          "arguments": [
            {
              "flags": "<id...>",
              "description": "Plugin Slugs, IDs, or File path"
            }
          ],
          "file": "./build/cli/index",
          "handler": "uninstallPlugins"
        },
        {
          "name": "plugin-disable",
          "description": "Disables one or more installed Plugins",
          "arguments": [
            {
              "flags": "<id...>",
              "description": "Plugin Slugs, IDs, or File path"
            }
          ],
          "file": "./build/cli/index",
          "handler": "disablePlugins"
        },
        {
          "name": "plugin-enable",
          "description": "Enables one or more installed Plugins",
          "arguments": [
            {
              "flags": "<id...>",
              "description": "Plugin Slugs, IDs, or File path"
            }
          ],
          "file": "./build/cli/index",
          "handler": "enablePlugins"
        },
        {
          "name": "plugin-update",
          "description": "Updates one or more installed Plugins to the latest compatible version",
          "arguments": [
            {
              "flags": "<id...>",
              "description": "Plugin Slugs, IDs, or File path"
            }
          ],
          "file": "./build/cli/index",
          "handler": "updatePlugins"
        }
      ]
    },
    "publisher": {
      "name": "TagoIO",
      "domain": "tago.io"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.5.6",
    "@apollo/react-hooks": "4.0.0",
    "@tago-io/tagocore-internal-sdk": "0.5.0",
    "@tago-io/tcore-api": "*",
    "@tago-io/tcore-console": "*",
    "@tago-io/tcore-sdk": "*",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "graphql": "16.3.0",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "semver": "7.3.5",
    "styled-components": "5.3.5",
    "typescript": "4.5.4"
  },
  "devDependencies": {
    "@types/node": "16.7.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "5.1.16",
    "@types/react-router-dom": "5.1.8",
    "@types/semver": "7.3.9",
    "@types/styled-components": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.5.1"
  }
}
