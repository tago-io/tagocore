{
  "name": "@tago-io/tcore-plugin-sqlite",
  "version": "0.7.0",
  "private": false,
  "main": "./build/index.js",
  "engines": {
    "tcore": ">= 0.7.0 < 0.8.0"
  },
  "files": [
    "build/**/*",
    "assets/**/*"
  ],
  "tcore": {
    "name": "SQLite",
    "short_description": "Integrates the SQLite Database with TagoCore",
    "full_description": "./README.md",
    "icon": "./assets/icon.png",
    "types": [
      "database"
    ],
    "permissions": [
      "action",
      "analysis",
      "device-data",
      "device"
    ],
    "publisher": {
      "name": "TagoIO",
      "domain": "tago.io"
    }
  },
  "scripts": {
    "watch": "tsc --watch",
    "build": "node esbuild/build",
    "migrate": "knex migrate:make --client=sqlite3 --migrations-directory=src/Migrations"
  },
  "dependencies": {
    "@tago-io/tcore-sdk": "0.7.2",
    "knex": "0.95.15",
    "sqlite3": "5.0.4",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/node": "16.7.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "esbuild": "^0.14.49",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1"
  }
}
